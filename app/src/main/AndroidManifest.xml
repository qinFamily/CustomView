<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="leavesc.hello.customview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="behind">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.clock.ClockViewActivity"
            android:screenOrientation="behind" />
        <activity
            android:name=".view.percentage.PercentageViewActivity"
            android:screenOrientation="behind" />
        <activity
            android:name=".view.circleRefresh.CircleRefreshViewActivity"
            android:screenOrientation="behind" />
        <activity
            android:name=".view.rain.RainViewActivity"
            android:screenOrientation="behind" />
        <activity
            android:name=".view.wave.WaveViewActivity"
            android:screenOrientation="behind" />
        <activity
            android:name=".view.taiji.TaiJiViewActivity"
            android:screenOrientation="behind" />
        <activity
            android:name=".view.waveLoading.WaveLoadingViewActivity"
            android:screenOrientation="behind" />
        <activity
            android:name=".view.pointBeat.PointBeatViewActivity"
            android:screenOrientation="behind" />
        <activity
            android:name=".view.plan.PlanViewActivity"
            android:screenOrientation="behind" />
    </application>

</manifest>